{
  "name": "@peculiarventures/pdf-doc",
  "version": "1.1.0",
  "description": "High level PDF module",
  "keywords": [],
  "author": "microshine <microshine@mail.ru>",
  "homepage": "https://github.com/PeculiarVentures/pdf/tree/master/packages/pdf-doc#readme",
  "license": "AGPL",
  "main": "build/peculiar.pdf.doc.js",
  "module": "build/peculiar.pdf.doc.es.js",
  "types": "build/types/index.d.ts",
  "files": [
    "build",
    "README.md"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PeculiarVentures/pdf.git"
  },
  "scripts": {
    "test": "echo \"Error: run tests from root\" && exit 1",
    "clear": "rimraf build/*",
    "build": "yarn run build:module && yarn run build:types",
    "build:module": "rollup -c",
    "prebuild:types": "rimraf build/types",
    "build:types": "tsc -p tsconfig.compile.json --outDir build/types --declaration --emitDeclarationOnly",
    "rebuild": "yarn run clear && yarn run build",
    "prepack": "npm run rebuild"
  },
  "bugs": {
    "url": "https://github.com/PeculiarVentures/pdf/issues"
  },
  "dependencies": {
    "@peculiarventures/pdf-core": "^1.1.0",
    "@peculiar/asn1-ocsp": "^2.0.38",
    "@peculiar/asn1-schema": "^2.0.38",
    "@peculiar/asn1-tsp": "^2.0.38",
    "@peculiar/asn1-x509": "^2.0.44",
    "@peculiar/x509": "^1.3.2",
    "asn1js": "^2.1.1",
    "bytestreamjs": "1.1.3",
    "fast-png": "^6.1.0",
    "pako": "^2.0.3",
    "pkijs": "^2.1.96",
    "pvtsutils": "^1.1.7"
  }
